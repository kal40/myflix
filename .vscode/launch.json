{
  "configurations": [
    {
      "name": "Launch Firefox debug",
      "type": "firefox",
      "request": "launch",
      "url": "http://localhost:1234/",
      "pathMappings": [
        {
          "url": "data:///__parcel_source_root",
          "path": "${workspaceFolder}"
        },
        {
          "url": "file:///__parcel_source_root/src",
          "path": "${workspaceFolder}/src"
        }
      ]
    }
  ]
}
